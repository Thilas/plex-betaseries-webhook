{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.validate": ["javascript", "typescript"],
  "jest.runMode": {
    "type": "on-save"
  },
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  "typescript.referencesCodeLens.enabled": true
}
