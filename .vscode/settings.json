{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.validate": ["javascript", "typescript"],
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-src-file"
  },
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  "typescript.referencesCodeLens.enabled": true
}
